{
  "name": "ecodrive",
  "version": "1.0.0",
  "private": true,
  "description": "API de Drive Pessoal no Bucket S3",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && node server.js",
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider && nodemon server.js -ext js,json,env,proto,prisma",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,scss,css,md}",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix"
  },
  "keywords": [
    "AWS",
    "Bucket",
    "S3",
    "Drive"
  ],
  "author": "Scient1st",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.799.0",
    "@aws-sdk/lib-storage": "^3.817.0",
    "@aws-sdk/s3-request-presigner": "^3.799.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@prisma/client": "^6.8.2",
    "axios": "^1.6.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "eslint-config-node": "^4.1.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "https": "^1.0.0",
    "husky": "^9.1.7",
    "json-bigint-patch": "^0.0.8",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.5.2",
    "nodemailer": "^7.0.3",
    "multer": "^2.0.0",
    "nodemon": "^3.0.1",
    "npm": "^10.8.2",
    "path": "^0.12.7",
    "short-unique-id": "^5.2.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0"
  }
}
